{
    "name": "symfony2admingenerator/twig-generator",
    "type": "library",
    "description": "A generator for PHP code based on Twig template engine",
    "keywords": ["twig", "generator", "code generation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cédric Lombardot",
            "email": "cedric.lombardot@gmail.com"
        },
        {
            "name": "François Zaninotto"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://github.com/symfony2admingenerator/TwigGenerator/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "twig/twig": "~1.40||~2.9||~3.0"
    },
    "require-dev": {
